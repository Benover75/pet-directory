{
  "name": "pet-directory",
  "version": "1.3.1",
  "description": "Pet Directory API",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "start:test": "cross-env NODE_ENV=test node index.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
    "test:e2e": "cypress run",
    "test:prepare": "cross-env NODE_ENV=test sequelize-cli --config config/config.js db:migrate:undo:all && cross-env NODE_ENV=test sequelize-cli --config config/config.js db:migrate && cross-env NODE_ENV=test sequelize-cli --config config/config.js db:seed:all",
    "test:unit": "jest --coverage --testPathPatterns=unit",
    "test:integration": "jest --coverage --testPathPatterns=integration",
    "test:load": "npx artillery run load-tests/basic.yml",
    "test:load:ci": "npx artillery run --output ./reports/load-report.json load-tests/smoke.yml",
    "test:contract": "npx openapi-diff --fail-on-changed ./openapi.json http://localhost:5000/api/v1"
  },
  "keywords": [],
  "author": "Lamberto Nunez",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.1.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "sequelize": "^6.37.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "artillery": "^2.0.0-37",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.0",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.2",
    "openapi-diff": "^0.24.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
}
